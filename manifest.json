{
  "name": "Vega Inspect",
  "version": "0.1",
  "description": "Extends the Developer Tools, adding a vega inspector.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "devtools_page": "devtools.html"
}
